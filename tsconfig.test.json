{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "module": "CommonJS",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "target": "es2022",
    "outDir": "dist",
    "strict": true,
    "skipLibCheck": true
  },
  "include": ["src/server/**/*"],
  "exclude": ["node_modules", "dist"]
} 